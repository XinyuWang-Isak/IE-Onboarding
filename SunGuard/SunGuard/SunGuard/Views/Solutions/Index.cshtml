@model IEnumerable<SunGuard.Models.Solution>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SolutionDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UVMin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UVMax)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SolutionDesc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UVMin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UVMax)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
